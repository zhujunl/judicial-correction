<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_text_white">


        <include
            android:id="@+id/tv_app_title"
            layout="@layout/layout_app_title"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btn_back_to_home"
            android:layout_width="236px"
            android:layout_height="85px"
            android:layout_marginTop="43px"
            android:layout_marginEnd="33px"
            android:background="@drawable/bg_btn_back_to_home"
            android:gravity="center"
            android:text="@string/btn_text_back_to_home"
            android:textColor="@color/color_text_btn"
            android:textSize="42px"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_app_title" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="128px"
            android:gravity="center"
            android:text="进度查询"
            android:textColor="@color/color_text_title"
            android:textSize="68px"
            android:textStyle="bold"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_app_title" />


        <View
            android:id="@+id/view_label_one"
            android:layout_width="11px"
            android:layout_height="41px"
            android:layout_marginStart="82px"
            android:layout_marginTop="83px"
            android:background="#2850A3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_title" />

        <TextView
            android:id="@+id/tv_progress_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="79px"
            android:text="进度信息"
            android:textColor="#393939"
            android:textSize="36px"
            app:layout_constraintLeft_toRightOf="@id/view_label_one"
            app:layout_constraintTop_toBottomOf="@id/tv_title" />


        <TextView
            android:id="@+id/tv_progress_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="58px"
            android:text="当前进度："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_one"
            app:layout_constraintTop_toBottomOf="@id/tv_progress_label" />

        <View
            android:id="@+id/view_label_two"
            android:layout_width="11px"
            android:layout_height="41px"
            android:layout_marginStart="82px"
            android:layout_marginTop="100px"
            android:background="#2850A3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_progress_content" />

        <TextView
            android:id="@+id/tv_apply_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="95px"
            android:text="居住地变更申请信息"
            android:textColor="#393939"
            android:textSize="36px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_progress_content" />


        <TextView
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="30px"
            android:text="姓名："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/view_label_two" />

        <TextView
            android:id="@+id/tv_id_card"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="身份证号："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_name" />

        <TextView
            android:id="@+id/tv_start_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="请假开始时间："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_id_card" />

        <TextView
            android:id="@+id/tv_end_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="请假结束时间："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_start_time" />

        <TextView
            android:id="@+id/tv_days"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="外出天数："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_end_time" />

        <TextView
            android:id="@+id/tv_reason_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="外出理由类型："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_days" />

        <TextView
            android:id="@+id/tv_reason"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="具体事由："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_days" />

        <TextView
            android:id="@+id/tv_temporary_guardian"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="临时监护人："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_reason" />

        <TextView
            android:id="@+id/tv_relationship"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="关系："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_temporary_guardian" />

        <TextView
            android:id="@+id/tv_phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="联系电话："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_relationship" />


        <TextView
            android:id="@+id/tv_application"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="请假申请书："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_phone" />

        <TextView
            android:id="@+id/tv_application_information"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="请假申请资料："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_application" />


        <View
            android:id="@+id/view_label_3"
            android:layout_width="11px"
            android:layout_height="41px"
            android:layout_marginStart="82px"
            android:layout_marginTop="100px"
            android:background="#2850A3"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_application_information" />

        <TextView
            android:id="@+id/tv_cancel_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="95px"
            android:text="销假信息"
            android:textColor="#393939"
            android:textSize="36px"
            app:layout_constraintLeft_toRightOf="@id/view_label_two"
            app:layout_constraintTop_toBottomOf="@id/tv_application_information" />


        <TextView
            android:id="@+id/tv_cancel_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="销假时间："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_3"
            app:layout_constraintTop_toBottomOf="@id/tv_cancel_title" />


        <TextView
            android:id="@+id/tv_out_proof"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="15px"
            android:layout_marginTop="12px"
            android:text="外出凭证："
            android:textColor="#393939"
            android:textSize="30px"
            app:layout_constraintLeft_toRightOf="@id/view_label_3"
            app:layout_constraintTop_toBottomOf="@id/tv_cancel_time" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>